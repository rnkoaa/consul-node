{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "globals": {
        "__DEV__": true
    },
    "testPathIgnorePatterns":["/node_modules/", "/helpers/"],
    "testRegex": "(./__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "rootDir": "."
}